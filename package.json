{
  "name": "dbs-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore --cache",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore --fix --cache",
    "type-check": "tsc",
    "snyk-test": "snyk test --severity-threshold=high",
    "snyk-test-auth": "snyk auth $SNYK_TOKEN && yarn snyk-test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@eGroupAI/hooks": "^8.33.26",
    "@eGroupAI/typings": "^8.36.31",
    "@eGroupAI/utils": "^8.33.21",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mui/icons-material": "^6.2.1",
    "@mui/material": "^6.2.0",
    "@reduxjs/toolkit": "^2.5.0",
    "axios": "^1.7.9",
    "dayjs": "^1.11.13",
    "mobile-detect": "^1.4.5",
    "next": "15.1.0",
    "query-string": "^9.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-markdown": "^9.0.1",
    "swr": "^2.2.5",
    "universal-cookie": "^7.2.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@eslint/js": "^9.19.0",
    "@types/cookie": "^1.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.19.0",
    "eslint-config-next": "15.1.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^15.14.0",
    "husky": ">=7",
    "lint-staged": "^15.4.3",
    "typescript": "^5",
    "typescript-eslint": "^8.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && lint-staged",
      "pre-push": "yarn lint && yarn snyk-test"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,json,md}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "eslint",
      "tsc-files globals.d.ts components/App/App.tsx --noEmit"
    ]
  }
}
