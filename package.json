{
  "name": "dbs-client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore --cache --max-warnings=-1",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --ignore-path .eslintignore --fix --cache --max-warnings=-1",
    "type-check": "tsc --noEmit",
    "snyk-test": "snyk test --severity-threshold=high",
    "snyk-test-auth": "snyk auth $SNYK_TOKEN && yarn snyk-test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@eGroupAI/hooks": "^8.33.26",
    "@eGroupAI/typings": "^8.36.31",
    "@eGroupAI/utils": "^8.33.21",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fortawesome/free-solid-svg-icons": "^6.7.1",
    "@fortawesome/react-fontawesome": "^0.2.2",
    "@mermaid-js/mermaid-mindmap": "^9.3.0",
    "@mui/icons-material": "^6.2.1",
    "@mui/material": "^6.2.0",
    "@mui/x-date-pickers": "^7.27.1",
    "@reduxjs/toolkit": "^2.5.0",
    "@types/js-cookie": "^3.0.6",
    "@types/mermaid": "^9.2.0",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "framer-motion": "^12.4.3",
    "js-cookie": "^3.0.5",
    "jwt-decode": "^4.0.0",
    "mermaid": "^11.4.1",
    "mobile-detect": "^1.4.5",
    "next": "15.1.0",
    "query-string": "^9.1.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-markdown": "^9.0.1",
    "svg-pan-zoom": "^3.6.2",
    "swr": "^2.2.5",
    "universal-cookie": "^7.2.2"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^12.0.8",
    "@types/cookie": "^1.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "^9.19.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-next": "15.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "27.6.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "typescript": "^5",
    "typescript-eslint": "^8.22.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn test && lint-staged",
      "pre-push": "yarn lint && yarn snyk-test"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix --cache --max-warnings=-1",
      "tsc-files globals.d.ts components/App/App.tsx --noEmit"
    ]
  }
}
